{
    "name": "B4UAgree",
    "version": "1.0.0",
    "description": "summary of privacy policy",
    "manifest_version": 3,
    "author": "Mia Hines",
    "action":{
        "default_popup": "index.html",
        "default_title": "Summary"
    }, 
    "permissions": ["storage", "activeTab", "declarativeContent"], 
    "content_scripts": [
        {
            "matches": ["<all_urls>"], 
            "js": ["jquery-3.3.1.js", "content.js"]
        }
    ]
}